{
  "name": "janitor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "icon": "react-native set-icon --path ./assets/ICON.png",
    "splash": "react-native set-splash --path ./assets/SPLASH.png --resize center",
    "splash-ios": "react-native set-splash --path ./assets/SPLASH.png --resize center --platform ios",
    "pod": "cd ./ios && pod install && cd ../",
    "keystore": "cd ./android/app && keytool -genkeypair -v -keystore my-upload-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000",
    "fingerprint": "keytool -list -v -keystore ./android/app/my-upload-key.keystore -alias my-key-alias -storepass bitwize -keypass bitwize",
    "fingerprint-debug": "keytool -list -v -keystore ./android/app/debug.keystore -alias androiddebugkey -storepass android -keypass android",
    "keyhash": "keytool -exportcert -alias MyFBKeyHash -keystore ./android/app/my-upload-key.keystore | openssl sha1 -binary | openssl base64",
    "keyhash-debug": "keytool -exportcert -alias MyFBDebugKeyHash -keystore ./android/app/debug.keystore | openssl sha1 -binary | openssl base64",
    "android-target": "react-native run-android --deviceId Pixel_3a_API_30",
    "emulators": "emulator -list-avds",
    "emulator": "~/Library/Android/sdk/emulator/emulator @Pixel_3a_API_30 -netfast -no-snapshot -memory 4096",
    "apk": "react-native run-android --variant=release",
    "aab": "cd ./android && ./gradlew bundleRelease",
    "clear-android": "cd ./android && ./gradlew clean",
    "server": "cd ../server && php artisan serve --host=0.0.0.0"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^7.1.7",
    "axios": "^0.24.0",
    "i18next": "^21.6.4",
    "libphonenumber-js": "^1.9.44",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moti": "^0.17.1",
    "prop-types": "^15.8.0",
    "react": "17.0.2",
    "react-i18next": "^11.15.3",
    "react-native": "0.66.4",
    "react-native-fast-image": "^8.5.11",
    "react-native-geolocation-service": "^5.3.0-beta.4",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-onesignal": "^4.3.3",
    "react-native-reanimated": "^2.3.1",
    "react-native-restart": "^0.0.23",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-sound": "^0.11.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-webview": "^11.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@bam.tech/react-native-make": "^3.0.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
